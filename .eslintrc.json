{
  "extends": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 90,
        "allowIndentationTabs": true
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}